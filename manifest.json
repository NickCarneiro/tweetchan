{
  "name": "tweetchan",
  "version": "1.0",
  "description": "Anonymize your twitter feed",
  "permissions": ["tabs", "*://*.twitter.com/*"],
  "content_scripts": [{
    "js": ["wordlist.js", "anonymize.js"],
    "css": ["anonymize.css"],
    "matches": ["https://twitter.com/"]
  }],
  "manifest_version": 2
}